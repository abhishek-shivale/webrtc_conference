{
  "name": "webrtc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "concurrently \"npm run dev:next\" \"npm run dev:server\"",
    "dev:next": "next dev --experimental-https",
    "build": "next build",
    "start": "next start",
    "dev:server": "nodemon --watch server --exec ts-node --project tsconfig.server.json server/server.ts",
    "lint": "next lint"
  },
  "dependencies": {
    "concurrently": "^9.2.0",
    "hls.js": "1.0.3-0.canary.7275",
    "mediasoup": "^3.18.0",
    "mediasoup-client": "^3.14.0",
    "next": "15.4.5",
    "nodemon": "^3.1.10",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "socket.io": "^4.8.1",
    "socket.io-client": "^4.8.1",
    "ts-node": "^10.9.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "15.4.5",
    "tailwindcss": "^4",
    "typescript": "^5"
  }
}
